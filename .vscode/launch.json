    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python: Current File",
            "type": "python",
            "cwd": "${workspaceFolder}/test",
            "request": "launch",
            "program": "crnf.py",
            "args": ["3"],
            "console": "integratedTerminal",
            "justMyCode": true
        },
        {
            "name": "26 debug",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/basics/bin/26",
            "args": [],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ],
            "preLaunchTask": "Build Task",
            "miDebuggerPath": "/usr/bin/gdb"
        },
    ]
}